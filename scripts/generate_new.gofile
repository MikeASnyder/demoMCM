package main

import (
	"encoding/json"

	"os"

	"github.com/rancher/types/apis/management.cattle.io/v3"
)

func main() {
	images := v3.AllK8sVersions
	data, err := json.Marshal(images)
	if err != nil {
		panic(err)
	}
	file, err := os.Create(".build/images.json")
	if err != nil {
		panic(err)
	}
	if _, err := file.Write(data); err != nil {
		panic(err)
	}
}
