  rancher_github_client_secret_hosted2 = "${var.rancher_github_client_secret_hosted2}"
  rancher_github_client_id_hosted3     = "${var.rancher_github_client_id_hosted3}"
  rancher_github_client_secret_hosted3 = "${var.rancher_github_client_secret_hosted3}"
  rke2_cluster_version_hosted1         = "${var.rke2_cluster_version_hosted1}"
  rke2_cluster_version_hosted2         = "${var.rke2_cluster_version_hosted2}"
  rke2_cluster_version_hosted3         = "${var.rke2_cluster_version_hosted3}"

  zone_id                              = "${var.zone_id}"
  docker_version                       = "${var.docker_version}"
  k3s_version_super                    = "${var.k3s_version_super}"
  k3s_version_hosted1                  = "${var.k3s_version_hosted1}"
  k3s_version_hosted2                  = "${var.k3s_version_hosted2}"
  k3s_version_hosted3                  = "${var.k3s_version_hosted3}"
  fullchain                            = "${var.fullchain}"
  privkey                              = "${var.privkey}"
}
