module "rancher_hosted" {
source                                 = "./modules/rancher-hosted"
  ssh_authorized_key                   = "${var.ssh_authorized_key}"
  ssh_private_key                      = "${var.ssh_private_key}"
  linode_root_password                 = "${var.linode_root_password}"
  linode_token                         = "${var.linode_token}"
  linode_resource_prefix               = "${var.linode_resource_prefix}"
  load_balancers_domain                = "${var.load_balancers_domain}"
  hosted1_load_balancer_subdomain      = "${var.hosted1_load_balancer_subdomain}"
  hosted2_load_balancer_subdomain      = "${var.hosted2_load_balancer_subdomain}"
  hosted3_load_balancer_subdomain      = "${var.hosted3_load_balancer_subdomain}"
  super_load_balancer_subdomain        = "${var.super_load_balancer_subdomain}"
  rancher_version_super                = "${var.rancher_version_super}"
  rancher_version_hosted1              = "${var.rancher_version_hosted1}"
  rancher_version_hosted2              = "${var.rancher_version_hosted2}"
  rancher_version_hosted3              = "${var.rancher_version_hosted3}"
  rancher_password                     = "${var.rancher_password}"
  rancher_github_client_id_super       = "${var.rancher_github_client_id_super}"
  rancher_github_client_secret_super   = "${var.rancher_github_client_secret_super}"
  rancher_github_client_id_hosted1     = "${var.rancher_github_client_id_hosted1}"
  rancher_github_client_secret_hosted1 = "${var.rancher_github_client_secret_hosted1}"
